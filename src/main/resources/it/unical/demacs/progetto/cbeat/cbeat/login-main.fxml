<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="640.0" styleClass="login" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unical.demacs.progetto.cbeat.cbeat.controller.MainLogin">
   <children>
      <Button fx:id="clientLoginBtn" mnemonicParsing="false" onMouseClicked="#showClientLogin" text="[login as client]">
         <VBox.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="workerLoginBtn" mnemonicParsing="false" onMouseClicked="#showWorkerLogin" text="[login as worker]">
         <VBox.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
